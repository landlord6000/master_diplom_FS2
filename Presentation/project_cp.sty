\def\@looser#1#2{\def\loos@space{\kern #1em}%
     \def\loos@text ##1{##1\loos@space\noexpand\loos@text}%
     \loos@text #2{\kern-#1em\let\loos@text=\relax
                   \catcode` =\the\@oldcatcode}\relax}



